   "source": [
    "def computegrade(score):\n",
    "  if score >= 0.9:\n",
    "    grade = 'A'\n",
    "  elif score >= 0.8:\n",
    "    grade = 'B'\n",
    "  elif score >= 0.7:\n",
    "    grade = 'C'\n",
    "  elif score >= 0.6:\n",
    "    grade = 'D'\n",
    "  else:\n",
    "    grade = 'F'\n",
    "  return grade\n",
    "\n",
    "computegrade(0.95)"
   ]
